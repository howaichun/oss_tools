 update `cms_posts` set `smeta`='{"thumb":"okaycms\/portal\/20170515\/59198d5089065.jpg","template":"","photo":[{"url":"okaycms\/portal\/20170515\/59198e6c239ea.png","alt":"\u4f20\u4f20\u4f20.png"}]}'  where `id`=67; 
